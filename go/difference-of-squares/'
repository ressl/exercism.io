package diffsquares

func SumOfSquares(number float64) float64 {
	sums := 0.0
	for i := 1.0; i <= 10; i++ {
		sums += math.Pow(i, 2)
	}
	return sums
}

func SquareOfSums(number float64) float64 {
	sums := 0.0
	for i := 1.0; i <= number; i++ {
		sums += i
	}
	return math.Pow(sums, 2)

}

